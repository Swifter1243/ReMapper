{
    "tags": {
        "allowUnknownTags": false
    },
    "source": {
        "include": "./src",
        "includePattern": "\\.ts$",
        "excludePattern": "(node_modules/|docs)"
    },
    "plugins": [
        "plugins/markdown",
        "jsdoc-plugin-typescript",
        "typescript",
        "plugins/typescript"
    ],
    "typescript": {
        "moduleRoot": "src"
    },
    "opts": {
        "template": "node_modules/docdash",
        "encoding": "utf8",
        "destination": "docs/",
        "recurse": true,
        "verbose": true
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}
